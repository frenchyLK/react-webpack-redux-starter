{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "React Starter Kit",
  "main": "src/index.jsx",
  "scripts": {
    "start": "webpack-dev-server --env.target dev --env.config local",
    "start:dev": "webpack-dev-server --env.target dev --env.config dev",
    "stats": "webpack --env.target analyze",
    "build:dev": "webpack --env.target build --env.config dev",
    "build:stage": "webpack --env.target build --env.config stage",
    "build:prod": "webpack --env.target build --env.config prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "serve:dev": "npm run build:dev && lite-server",
    "storybook": "start-storybook -p 9001 .storybook"
  },
  "author": "John Benson, Samantha Routledge, David Legare",
  "engines": {
    "node": ">=4.7.0",
    "npm": ">=4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|gif|png|mp4|mkv|avi|webm|swf|wav|mid)$": "jest-static-stubs/$1",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./test/setup.js"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "~3.2.13",
    "@storybook/react": "~3.2.13",
    "babel-core": "~6.25.0",
    "babel-eslint": "~7.2.3",
    "babel-jest": "~20.0.3",
    "babel-loader": "~7.1.1",
    "babel-plugin-transform-imports": "~1.4.1",
    "babel-polyfill": "~6.26.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-3": "~6.24.1",
    "clean-webpack-plugin": "~0.1.16",
    "compression": "~1.7.1",
    "connect-history-api-fallback": "~1.4.0",
    "css-loader": "~0.28.4",
    "enzyme": "~3.1.1",
    "enzyme-adapter-react-16": "~1.0.4",
    "enzyme-matchers": "~4.0.1",
    "eslint": "~4.3.0",
    "eslint-loader": "~1.9.0",
    "eslint-plugin-jest": "~21.2.0",
    "eslint-plugin-react": "~7.4.0",
    "extract-text-webpack-plugin": "~3.0.0",
    "file-loader": "~0.11.2",
    "html-webpack-plugin": "~2.29.0",
    "html-webpack-template": "~5.6.0",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~21.0.1",
    "jest-static-stubs": "~0.0.1",
    "json-loader": "~0.5.7",
    "lite-server": "~2.3.0",
    "node-sass": "~4.5.3",
    "raf": "~3.4.0",
    "sass-loader": "~6.0.6",
    "style-loader": "~0.18.2",
    "url-loader": "~0.5.9",
    "webpack": "~3.5.3",
    "webpack-bundle-analyzer": "~2.8.3",
    "webpack-dev-server": "~2.7.1",
    "webpack-merge": "~4.1.0"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "~1.21.0",
    "class-autobind": "~0.1.4",
    "classnames": "~2.2.5",
    "i18next": "~8.4.3",
    "i18next-xhr-backend": "~1.4.2",
    "immutable": "~3.8.1",
    "normalizr": "~3.2.3",
    "react": "~16.0.0",
    "react-dom": "~16.0.0",
    "react-hot-loader": "~3.1.1",
    "react-i18next": "~4.7.0",
    "react-immutable-proptypes": "~2.1.0",
    "react-immutable-render-mixin": "~0.9.7",
    "react-redux": "~5.0.5",
    "react-router-dom": "~4.2.2",
    "react-router-redux": "~5.0.0-alpha.8",
    "redux": "~3.7.2",
    "redux-actions": "~2.2.1",
    "redux-form": "~7.1.2",
    "redux-immutable": "~4.0.0",
    "redux-normalizr-middleware": "~2.0.0",
    "redux-saga": "~0.15.6",
    "redux-saga-routines": "~1.1.2",
    "regenerator-runtime": "~0.11.0",
    "reselect": "~3.0.1",
    "spinkit": "~1.2.5"
  }
}
